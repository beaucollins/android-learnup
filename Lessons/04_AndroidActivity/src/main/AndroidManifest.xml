<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.automattic.learnup" >

    <application
        android:allowBackup="false"
        android:icon="@android:drawable/sym_def_app_icon" >
        <activity
            android:name="com.automattic.learnup.Activity1"
            android:label="@string/title_activity_activity1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.automattic.learnup.ActivityLightStyle"
            android:label="@string/title_activity_activity_light_style"
            android:parentActivityName="com.automattic.learnup.Activity1"
            android:theme="@android:style/Theme.Holo.NoActionBar.Fullscreen"
            tools:ignore="UnusedAttribute" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.automattic.learnup.Activity1" />
        </activity>
        <activity
            android:name="com.automattic.learnup.MyActivity"
            android:label="@string/title_activity_my" >
        </activity>
    </application>

</manifest>