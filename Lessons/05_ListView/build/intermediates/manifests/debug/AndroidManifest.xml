<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.automattic.learnup_listview"
    android:versionCode="1" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="20" />

    <application
        android:allowBackup="false"
        android:icon="@android:drawable/sym_def_app_icon" >
        <activity
            android:name="com.automattic.learnup_listview.SimpleListActivity"
            android:label="@string/title_activity_simple_list" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.automattic.learnup_listview.ComplexListActivity"
            android:label="@string/title_activity_complex_list" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>